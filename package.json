{
  "name": "pinniped",
  "version": "0.1.5",
  "description": "Easy-to-deploy Node backend server.",
  "keywords": [
    "backend",
    "sqlite3",
    "node"
  ],
  "main": "./src/Pinniped/Pinniped.js",
  "type": "module",
  "scripts": {
    "start": "node index.js",
    "prod": "node ./dist/Pinniped.js",
    "dev": "nodemon index.js",
    "reset-db": "rm -f pnpd_data/pnpd.db && touch pnpd_data/pnpd.db && npm run seed-data",
    "reset-sessions": "rm -f pnpd_data/session.db && touch pnpd_data/session.db",
    "reset-migrations": "rm -r pnpd_data/migrations && sqlite3 pnpd_data/pnpd.db < pnpd_data/drop-migrations.sql",
    "seed-data": "sqlite3 pnpd_data/pnpd.db < pnpd_data/seed-data.sql",
    "build": "npx parcel build index.js && npm run build-cleanup",
    "build-cleanup": "rm -rf .parcel-cache"
  },
  "contributors": [
    {
      "name": "Alan Cho",
      "email": "alancho397@gmail.com"
    },
    {
      "name": "Jonathan Hurd",
      "email": "hurd.jonathan@gmail.com"
    },
    {
      "name": "Josh Lakenan",
      "email": "joshlakenan@gmail.com"
    },
    {
      "name": "Trevor Rees-Jones",
      "eail": "treesjones3@gmail.com"
    }
  ],
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "better-sqlite3": "^9.4.3",
    "better-sqlite3-session-store": "^0.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.3",
    "express-session": "^1.18.0",
    "knex": "^3.1.0",
    "pinniped": "^0.1.4",
    "uuid": "^9.0.1",
    "xss": "^1.0.15"
  },
  "devDependencies": {
    "nodemon": "^3.1.0",
    "parcel": "^2.12.0"
  }
}
