{
  "name": "pinniped",
  "version": "0.2.2",
  "description": "Easy-to-deploy Node backend server.",
  "keywords": [
    "backend",
    "sqlite3",
    "node",
    "baas"
  ],
  "scripts": {
    "start": "node index.js",
    "prod": "node ./dist/Pinniped.js",
    "dev": "nodemon index.js",
    "reset-all": "npm run reset-sessions && npm run reset-migrations && npm run reset-db",
    "reset-db": "rm -f pnpd_data/pnpd.db && touch pnpd_data/pnpd.db && rm -f pnpd_data/pnpd.db-wal && rm -f pnpd_data/pnpd.db-shm",
    "reset-sessions": "rm -f pnpd_data/session.db && touch pnpd_data/session.db",
    "reset-migrations": "rm -r pnpd_data/migrations && sqlite3 pnpd_data/pnpd.db < pnpd_data/drop-migrations.sql",
    "build": "npx parcel build",
    "build-cleanup": "rm -rf .parcel-cache"
  },
  "contributors": [
    {
      "name": "Alan Cho",
      "email": "alancho397@gmail.com"
    },
    {
      "name": "Jonathan Hurd",
      "email": "hurd.jonathan@gmail.com"
    },
    {
      "name": "Josh Lakenan",
      "email": "joshlakenan@gmail.com"
    },
    {
      "name": "Trevor Rees-Jones",
      "eail": "treesjones3@gmail.com"
    }
  ],
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "better-sqlite3": "^9.4.3",
    "better-sqlite3-session-store": "^0.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.3",
    "express-session": "^1.18.0",
    "knex": "^3.1.0",
    "pinniped": "^0.2.1",
    "xss": "^1.0.15"
  },
  "devDependencies": {
    "nodemon": "^3.1.0"
  },
  "type": "module",
  "main": "./dist/Pinniped.js",
  "source": "index.js",
  "targets": {
    "main": {
      "outputFormat": "esmodule",
      "includeNodeModules": true
    }
  }
}
